{% extends 'baseadmin.html.twig' %}

{% block title %}Features{% endblock %}

{% block body %}
<div class="show">
    <h1>Features</h1>
    <br>
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ feature.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ feature.Title }}</td>
            </tr>
            <tr>
                <th>Text</th>
                <td>{{ feature.Text }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('features_index') }}">back to list</a>

    <a href="{{ path('features_edit', {'id': feature.id}) }}">edit</a>

    {{ include('features/_delete_form.html.twig') }}
</div>
{% endblock %}
