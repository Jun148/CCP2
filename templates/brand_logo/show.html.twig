{% extends 'baseadmin.html.twig' %}

{% block title %}BrandLogo{% endblock %}

{% block body %}
<div class="show">
    <h1>BrandLogo</h1>
    <br>
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ brand_logo.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ brand_logo.Name }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td><img src="{{ vich_uploader_asset(brand_logo.image, 'imageFile') }}"></td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('brand_logo_index') }}">back to list</a>

    <a href="{{ path('brand_logo_edit', {'id': brand_logo.id}) }}">edit</a>

    {{ include('brand_logo/_delete_form.html.twig') }}
</div>
{% endblock %}
